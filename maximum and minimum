#include <iostream>
using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5};  
    int size = 5;
    int max=arr[0];
    int min=arr[0];
    for(int i=0;i<size;i++){
    	cout<<arr[i]<<" ";
	}
    for(int i=0;i<size;i++){
    	if(arr[i]<min ) min=arr[i];
    	if(arr[i]>max) max=arr[i];
	}
	cout<<"\nmax is "<<max<<endl;
	cout<<"min is "<<min<<endl;

    return 0;
}
