#include <iostream>
using namespace std;
int main() {
    int arr[4] = {10 ,20 , 23 ,60};
    cout<<"Original array is "<<endl;
    
    for(int i=0;i<4;i++){
    	cout<<arr[i];
	}
 cout<<" \first  index i want to delete "<<endl;
 for(int i=1;i<4;i++){
 	arr[i]=arr[i+1];
 }
    
    cout << "\nupdated array is  "<<endl;
    for (int i = 0; i < 3; i++) { 
        cout << arr[i] << " ";
    }

    return 0;
}
