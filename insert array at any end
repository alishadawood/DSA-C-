#include<iostream>
using namespace std;
int main(){
	int arr[10];
	int size=5;
	int value;
	cout<<"Enter the array element"<<endl;
	for(int i=0;i<size;i++){
		cin>>arr[i];
	}
	cout<<"Enter the number that you want to add at the end"<<endl;
	
	cin>>value;
	arr[size]=value;
	size++;
	cout<<"Updated array"<<endl;
	for(int i=0;i<size;i++)
	{
		cout<<arr[i]<<endl;
		
	}
	return 0;
	}
