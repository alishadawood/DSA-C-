#include<iostream>
using namespace std;
int main(){
	int arr[10]={1,2,3,4,5};
	int size=5;
	int value=0;
	cout<<"original value"<<endl;
	for(int i=0;i<size;i++){
		cout<<arr[i]<<" ";
	}
	for(int i=size;i>0;i--){
		arr[i]=arr[i-1];
	}
	 arr[0]=value;
	size++;
	cout<<"\nafter insertion "<<endl;
	for(int i=0;i<size;i++){
		cout<<arr[i]<<" ";
	}
	return 0;
}
