#include <iostream>
using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5};  
    int size = 5;
    cout<<"original array"<<endl;
	for(int i = 0; i < 5; i++){
		cout<<arr[i];
	}  

    cout << "\nReversed Array: ";
   for(int i=size-1;i>=0;i--) {  
        cout << arr[i] << " ";  
    }

    return 0;
}
