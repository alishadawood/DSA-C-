#include <iostream>
using namespace std;
int main() {
    int arr[4] = {10 ,20 , 23 ,60};
    cout<<"Original array is "<<endl;
    
    for(int i=0;i<4;i++){
    	cout<<arr[i];
	}
arr[2]=9;
    
    cout << "\nupdated array is  "<<endl;
    for (int i = 0; i < 4; i++) { 
        cout << arr[i] << " ";
    }

    return 0;
}
