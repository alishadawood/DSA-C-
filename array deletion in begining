#include<iostream>
using namespace std;
int main(){
	int arr[10]={1,2,3,4,5};
	int size=5;
	cout<<"original array element"<<endl;
	for(int i=0;i<size;i++){
		cout<<arr[i]<<" ";}
	for(int i=0;i<size-1;i++){
		arr[i]=arr[i+1];
	}
	size--;
	cout<<"\nAfter deletion in begining"<<endl;
	for(int i=0;i<size;i++){
		cout<<arr[i]<<" ";
	}
	return 0;
}
