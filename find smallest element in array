#include<iostream>
using namespace std;
int main(){
int arr[10];
int size=5;
cout<<"enter "  <<size<<" element "<<endl;
for(int i=0;i<size;i++){
	cin>>arr[i];
}
int smallest=arr[0];
for(int i=1;i<size;i++){
if(arr[i]<smallest){
	smallest=arr[i];
}	

}
cout<<"smallest element is  "<<smallest<<endl;
	return 0;
	}
